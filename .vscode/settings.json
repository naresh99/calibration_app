{
	"files.exclude": {
		"amplify/.config": true,
		"amplify/**/*-parameters.json": true,
		"amplify/**/amplify.state": true,
		"amplify/**/transform.conf.json": true,
		"amplify/#current-cloud-backend": true,
		"amplify/backend/amplify-meta.json": true,
		"amplify/backend/awscloudformation": true
	}
,

"deploy": {
  "packages": [
      {
          "name": "Version 2.3.4",
          "description": "Package version 2.3.4",
          "files": [
            "mch_sch_rest/*.py"
        ],
          "exclude": [
            "mch_sch_rest/test/**",
            "mch_sch_rest/mch_sch_lambda.py",
            "mch_sch_rest/schedule_lambda.py"
        ],
          "deployOnSave": false
      },
      {
          "name": "Version 2.3.5 (anything)",
          "description": "Package version 2.3.5"
      }
  ],

  "targets": [
      {
        "type": "zip",
        "name": "My ZIP file",
        "description": "Create a ZIP file in a target directory",
        "open": false,
        "target": "D:/"
      }
]

}
}
